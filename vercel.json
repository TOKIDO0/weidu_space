{
  "buildCommand": "npm run build",
  "outputDirectory": "admin/.next",
  "installCommand": "cd admin && npm install",
  "framework": "nextjs",
  "rewrites": [
    { "source": "/", "destination": "/public/index.html" },
    { "source": "/(.*)", "destination": "/public/$1" }
  ],
  "crons": [
    {
      "path": "/api/schedule-notifications",
      "schedule": "0 8 * * *"
    }
  ]
}







