{
  "buildCommand": "cd admin && npm install && npm run build",
  "outputDirectory": "admin/.next",
  "installCommand": "cd admin && npm install",
  "framework": "nextjs",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/index.html", "destination": "/" },
    { "source": "/projects.html", "destination": "/projects.html" },
    { "source": "/project-track.html", "destination": "/project-track.html" },
    { "source": "/ai-assistant.html", "destination": "/ai-assistant.html" },
    { "source": "/terms.html", "destination": "/terms.html" }
  ],
  "crons": [
    {
      "path": "/api/schedule-notifications",
      "schedule": "0 8 * * *"
    }
  ]
}







